<h1>Sign Up</h1>

<%= form_for @user, :url => users_path, :html => {:multipart => true} do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.file_field :avatar %>
  <%= f.submit "Sign Up" %>
<% end %>
